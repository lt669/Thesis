@article{Schroeder1963,
abstract = {This paper describes a method for evaluating the acoustics of concert halls, prior to construction, by simulating the sound transmission in the hall on a digital computer. A reverberation-free sound signal is sampled, quantized and fed into the computer. The computer adds echoes and reverberation to this signal and produces two output signals. These signals, when radiated from two loudspeakers in an anechoic chamber, combine at the ears of a centrally seated listener to form echo responses similar to those in the real hall. The computer program allows the specification of the individual discrete echoes in amplitude, delay and direction of incidence. The continuous reverberation is simulated with adjustable frequency-dependent reverberation time, shape of decay curve, diffusion, echo density and frequency response irregularity. [full article in: IEEE International Convention (New York, NY, USA March 25-28,1963]},
