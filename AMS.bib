@article{Blauert2000,
abstract = {A system is described which is capable of presenting complex auditory environments to listeners in a perceptually plausible way, such that the listeners feel actually present in them. The listeners can move around in the presented environment and interact with it, e.g., by hand-movements. The virtual environment may encompass multiple direct and reflected sounds from spatially nonstationary sources, their actual number being only limited by current hardware restrictions. The setting of the sound paths (direct and reflected) can be cast after real acoustic scenarios or in any other reasonable way. Consequently, the system forms a flexible tool for psychoacoustic experimentation - among other applications. The system architecture has been designed in such a way that perceptual representation of other modalities (e.g., visual, tactile/thermal) can easily be incorporated. In fact, it represents the kernel of a multimodal virtual-reality generator. In psychoacoustic research this feature allows for providing feedback to non-auditory modalities.},
author = {Blauert, Jens and Lehnert, Hilmar and Sahrhage, Jorg and Strauss, Holger},
file = {:Users/Lewis/Uni Work/4th Year/Second term/AMS/3/blauert2000.pdf:pdf},
isbn = {0001-7884},
issn = {00017884},
journal = {Acta Acustica},
pages = {94--102},
title = {{An Interactive Virtual-Environment Generator for Psychoacoustic Research I: Architecture and Implementation}},
year = {2000}
}
@article{Bonham1999,
abstract = {A commonly accepted physiological model for lateralization of low-frequency$\backslash$nsounds by interaural time delay (ITD) stipulates that binaural comparison$\backslash$nneurons receive input from frequency-matched channels from each ear.$\backslash$nHere, the effects of hypothetical interaural frequency mismatches$\backslash$non this model are reported. For this study, the cat's auditory system$\backslash$nperipheral to the binaural comparison neurons was represented by$\backslash$na neurophysiologically derived model, and binaural comparison neurons$\backslash$nwere represented by cross-correlators. The results of the study indicate$\backslash$nthat, for binaural comparison neurons receiving input from one cochlear$\backslash$nchannel from each ear, interaural CF mismatches may serve to either$\backslash$naugment or diminish the effective difference in ipsilateral and contralateral$\backslash$naxonal time delays from the periphery to the binaural comparison$\backslash$nneuron. The magnitude of this increase or decrease in the effective$\backslash$ntime delay difference can be up to 400 microseconds for CF mismatches$\backslash$nof 0.2 octaves or less for binaural neurons with CFs between 250$\backslash$nHz and 2.5 kHz. For binaural comparison neurons with nominal CFs$\backslash$nnear 500 Hz, the 25-microsecond effective time delay difference caused$\backslash$nby a 0.012-octave CF mismatch is equal to the ITD previously shown$\backslash$nto be behaviorally sufficient for the cat to lateralize a low-frequency$\backslash$nsound source.},
author = {Bonham, B H and Lewis, E R},
doi = {10.1121/1.427056},
file = {:Users/Lewis/Uni Work/4th Year/Second term/AMS/Report/Papers/ITD.pdf:pdf},
issn = {00014966},
journal = {J Acoust Soc Am},
keywords = {Afferent,Animals,Axons,Biological,Cats,Cochlea,Hearing,Models,Neurons,physiology},
number = {1},
pages = {281--290},
pmid = {10420622},
title = {{Localization by interaural time difference (ITD): effects of interaural frequency mismatch.}},
volume = {106},
year = {1999}
}
